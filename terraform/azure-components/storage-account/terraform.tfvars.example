name                = "stsampleeastus001"
resource_group_name = "rg-sample-eastus-001"
location            = "eastus"
account_tier        = "Standard"
account_replication_type = "LRS"
account_kind        = "StorageV2"
access_tier         = "Hot"
min_tls_version     = "TLS1_2"
enable_https_traffic_only = true
allow_nested_items_to_be_public = false

network_rules = {
  default_action             = "Deny"
  ip_rules                   = []
  virtual_network_subnet_ids = ["/subscriptions/00000000-0000-0000-0000-000000000000/resourceGroups/rg-sample-eastus-001/providers/Microsoft.Network/virtualNetworks/vnet-sample-eastus-001/subnets/snet-sample-app-001"]
  bypass                     = ["AzureServices"]
}

tags = {
  Environment = "Development"
  ManagedBy   = "Terraform"
}
